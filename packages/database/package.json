{
  "name": "@pixel-flow/database",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./balance": {
      "types": "./dist/balance.d.ts",
      "import": "./dist/balance.js",
      "require": "./dist/balance.js",
      "default": "./dist/balance.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev --name init --schema=prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0"
  },
  "devDependencies": {
    "prisma": "^5.17.0",
    "typescript": "^5.4.5"
  }
}
